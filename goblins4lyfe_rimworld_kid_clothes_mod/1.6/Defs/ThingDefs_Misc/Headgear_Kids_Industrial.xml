<Defs>
	<!-- ##### Industrial ##### -->
	<!-- +++++ Kid Flak Helmet +++++ -->
	<ThingDef ParentName="ArmorHelmetMakeableBase">
 <defName>Apparel_Goblins4Lyfe_AdvancedKidHelmet</defName>
    <label>kid flak helmet</label>
    <description>A lightweight combat helmet with a complex design, reinforced with plasteel. For the especially valuable conscript.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
      <displayPriority>205</displayPriority>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/KidFlakHelmet/KidFlakHelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>1.4</uiIconScale>
    <statBases>
      <WorkToMake>6400</WorkToMake>
      <MaxHitPoints>120</MaxHitPoints>
      <Mass>0.6</Mass>
      <StuffEffectMultiplierArmor>0.7</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.15</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
    <costList>
      <ComponentIndustrial>1</ComponentIndustrial>
      <Plasteel>5</Plasteel>
    </costList>
    <apparel>
    <developmentalStageFilter>Child</developmentalStageFilter>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/KidFlakHelmet/KidFlakHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>IndustrialMilitaryAdvanced</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0.33,0.33,0.33,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.65,0.65,0.65,1)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(0.3,0.3,0.3,1)</min>
          <max>(0.5,0.5,0.5,1)</max>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.9,0.9,0.9,1)</only>
        </li>
      </options>
    </colorGenerator>
    <tradeTags>
      <li>Armor</li>
    </tradeTags>
  </ThingDef>
	<!-- +++++ Kid Foil Helmet +++++ -->
	<ThingDef ParentName="HatBase">
    <defName>Apparel_Goblins4Lyfe_KidPsychicFoilHelmet</defName>
    <label>kid psychic foil helmet</label>
    <description>A thin helmet containing finely-tuned sheets of psychically-absorptive foil. To protect your children from government psycasters.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/KidPsychicFoilHelmet/KidPsychicFoilHelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>1.3</uiIconScale>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <MarketValue>250</MarketValue>
      <Mass>0.5</Mass>
      <Flammability>0.4</Flammability>
      <ArmorRating_Sharp>0.09</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.09</ArmorRating_Blunt>
      <ArmorRating_Heat>0.27</ArmorRating_Heat>
      <Insulation_Cold>2</Insulation_Cold>
      <Insulation_Heat>1</Insulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <PsychicSensitivity>-0.90</PsychicSensitivity>
    </equippedStatOffsets>
    <generateCommonality>0.1</generateCommonality>
    <smeltable>false</smeltable>
    <apparel>
	<developmentalStageFilter>Child</developmentalStageFilter>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/KidPsychicFoilHelmet/KidPsychicFoilHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>IndustrialMilitaryBasic</li>
        <li>IndustrialMilitaryAdvanced</li>
        <li>SpacerMilitary</li>
      </tags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0.33,0.33,0.33,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.65,0.65,0.65,1)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(0.3,0.3,0.3,1)</min>
          <max>(0.5,0.5,0.5,1)</max>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.9,0.9,0.9,1)</only>
        </li>
      </options>
    </colorGenerator>
    <tradeTags>
      <li>Clothing</li>
    </tradeTags>
    <thingSetMakerTags>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
  </ThingDef>
