	<!-- +++++ Kid Gunlink +++++ -->
	<ThingDef ParentName="ApparelArmorHelmetReconBase">
    <defName>Apparel_Goblins4Lyfe_KidGunlink</defName>
    <label>learnlink</label>
    <description>A learning assistant that helps the user to learn. It analyzes targets using infrared, visible, and millimeter-wave electromagnetic sensors. An onboard computer displays educational facts on a retinal projection. It offers no protection at all.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/KidGunlink/KidGunlink</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <equippedStatOffsets>
      <LearningRateFactor>0.5</LearningRateFactor>
      <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0</VacuumResistance>
    </equippedStatOffsets>
    <statBases>
      <ArmorRating_Sharp>0</ArmorRating_Sharp>
      <ArmorRating_Blunt>0</ArmorRating_Blunt>
      <ArmorRating_Heat>0</ArmorRating_Heat>
      <Insulation_Cold>0</Insulation_Cold>
      <Insulation_Heat>0</Insulation_Heat>
    </statBases>
    <recipeMaker>
      <researchPrerequisite>Gunlink</researchPrerequisite>
      <displayPriority>400</displayPriority>
    </recipeMaker>
    <costList>
      <ComponentSpacer>1</ComponentSpacer>
      <Plasteel>5</Plasteel>
    </costList>
    <apparel>
	<developmentalStageFilter>Child</developmentalStageFilter>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/KidGunlink/KidGunlink</wornGraphicPath>
      <renderSkipFlags>
        <li>None</li>
      </renderSkipFlags>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups Inherit="False">
        <li>UpperHead</li>
      </bodyPartGroups>
      <tags Inherit="False">
        <li>Gunlink</li>
      </tags>
      <defaultOutfitTags Inherit="False">
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <tradeTags>
      <li>Clothing</li>
    </tradeTags>
    <thingSetMakerTags Inherit="False">
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
    <comps Inherit="False">
      <li Class="CompProperties_Biocodable"/>
      <li Class="CompProperties_Forbiddable"/>
      <li Class="CompProperties_Styleable" />
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
  </ThingDef>
	<!-- +++++ Kid Slicecap +++++ -->
	<ThingDef ParentName="HatMakeableBase">
    <defName>Apparel_Goblins4Lyfe_KidSlicecap</defName>
    <label>kid slicecap</label>
    <description>A hard cap that partly covers the wearer's scalp. Most children view this headpiece as uncomfortable and silly.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/KidSliceCap/KidSliceCap</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>1600</WorkToMake>
      <MaxHitPoints>80</MaxHitPoints>
      <Mass>0.4</Mass>
      <EquipDelay>1.5</EquipDelay>
      <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.1</StuffEffectMultiplierInsulation_Heat>
    </statBases>
    <stuffCategories>
      <li>Woody</li>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>10</costStuffCount>
    <apparel>
    <developmentalStageFilter>Child</developmentalStageFilter>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPaths>
        <li>Things/Pawn/Humanlike/Apparel/SliceCap/SliceCapA</li>
        <li>Things/Pawn/Humanlike/Apparel/SliceCap/SliceCapB</li>
        <li>Things/Pawn/Humanlike/Apparel/SliceCap/SliceCapC</li>
      </wornGraphicPaths>
      <layers>
        <li>Overhead</li>
      </layers>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
      <canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
      <ideoDesireDisallowedFactionCategoryTags><li>Tribal</li></ideoDesireDisallowedFactionCategoryTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
    <recipeMaker>
      <displayPriority>140</displayPriority>
    </recipeMaker>
  </ThingDef>
