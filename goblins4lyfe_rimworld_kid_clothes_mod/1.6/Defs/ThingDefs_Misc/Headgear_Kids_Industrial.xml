	<!-- +++++ Kid Blindfold +++++ -->
	<ThingDef ParentName="HatMakeableBase">
    <defName>Apparel_Goblins4Lyfe_KidBlindfold</defName>
    <label>kid blindfold</label>
    <description>A long strip of cloth that is wrapped over the eyes to block vision. Children have a habit of peeking out of it, even when told not to.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/KidBlindfold/KidBlindfold</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(0.8, 0.8)</drawSize>
    </graphicData>
    <costStuffCount>10</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
    </stuffCategories>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToMake>960</WorkToMake>
      <Mass>0.035</Mass>
      <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.35</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
      </recipeUsers>
      <memePrerequisitesAny><li>Blindsight</li></memePrerequisitesAny>
    </recipeMaker>
    <apparel>
    <developmentalStageFilter>Child</developmentalStageFilter>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>Eyes</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/KidBlindfold/KidBlindfold</wornGraphicPath>
      <layers>
        <li>EyeCover</li>
      </layers>
      <drawData>
        <defaultData>
          <layer>61</layer>
        </defaultData>
      </drawData>
      <renderSkipFlags>
        <li>None</li>
      </renderSkipFlags>
      <blocksVision>true</blocksVision>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
      <canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
    </apparel>
    <comps>
      <li Class="CompProperties_CauseHediff_Apparel">
        <hediff>Blindfold</hediff>
        <part>Head</part>
      </li>
    </comps>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
